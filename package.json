{
  "name": "sz-rat-app",
  "version": "0.6.6",
  "sz_rat": {
    "version": "0.6.6",
    "vname":"Lee",
    "description":"Seymour Zhang's React Application Template",
    "repo":"https://github.com/zsh2401/sz-rat"
  },
  "description": "An application based powered by sz-rat",
  "main": "./scripts/server",
  "scripts": {
    "serv": "ts-node --project ./scripts/server/tsconfig.json  ./scripts/server",
    "deploy": "node ./scripts/deploy.js",
    "build": "cross-env TS_NODE_PROJECT=\"./scripts/tsconfig-for-webpack-config.json\" webpack --config ./scripts/webpack.config.prod.ts",
    "build:dev": "cross-env TS_NODE_PROJECT=\"./scripts/tsconfig-for-webpack-config.json\" webpack --config ./scripts/webpack.config.dev.ts",
    "start": "cross-env TS_NODE_PROJECT=\"./scripts/tsconfig-for-webpack-config.json\" webpack-dev-server --config ./scripts/webpack.config.dev.ts",
    "start:prod": "cross-env TS_NODE_PROJECT=\"./scripts/tsconfig-for-webpack-config.json\" webpack-dev-server --config ./scripts/webpack.config.prod.ts"
  },
  "keywords": [
    "react",
    "react-router",
    "SPA",
    "PWA"
  ],
  "author": "zsh2401",
  "license": "MIT",
  "devDependencies": {
    "@types/connect-history-api-fallback": "^1.3.3",
    "@types/copy-webpack-plugin": "^5.0.0",
    "@types/express": "^4.17.2",
    "@types/gh-pages": "^2.0.1",
    "@types/html-webpack-plugin": "^3.2.1",
    "@types/lodash": "^4.14.147",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.3",
    "@types/react-loadable": "^5.5.2",
    "@types/react-router-dom": "^5.1.0",
    "@types/uglifyjs-webpack-plugin": "^1.1.0",
    "@types/webpack": "^4.39.5",
    "@types/webpack-bundle-analyzer": "^2.13.3",
    "awesome-typescript-loader": "^5.2.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.0.0",
    "connect-history-api-fallback": "^1.6.0",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "ejs-loader": "^0.3.5",
    "express": "^4.17.1",
    "file-loader": "^4.2.0",
    "gh-pages": "^2.1.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.12.0",
    "popper.js": "^1.16.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.6.4",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^2.2.0",
    "webpack": "4.39.3",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "3.7.2",
    "webpack-parallel-uglify-plugin": "^1.1.2"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "jquery": "^3.4.1",
    "lodash": "^4.17.15",
    "offline-plugin": "^5.0.7",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-router-dom": "^5.1.2",
    "react-use": "^13.8.0"
  }
